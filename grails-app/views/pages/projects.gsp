	<!-- start: Container -->
	<div class="container">

		<br />
		<div class="row">
			<div class="span12">
				<h4 class="heading">Recent <strong>projects</strong> people in the community are building</h4>
				<div class="row">
					<section id="projects">
						<ul id="thumbs" class="portfolio">
